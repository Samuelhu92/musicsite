/* HOME PAGE */


.main {
  overflow: hidden;
  width: 100%;
  position: relative;
  z-index: 1;
  top: -200px;
  margin-bottom: -200px;
  height:2000px;

}
/* full screen video*/
.section {
  position: relative;
  z-index: 1;
  width: 100%;
}

.section-fullscreen{
background-color: #f8f8ff;
color: #ccc;
min-height: 800px;
}

.page-content .video-fade{
  position: fixed;
}
.page-content .section-fullscreen .section-background{
z-index: -1;

}
.page-content .section-fullscreen .section-dance{
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  max-width: none;
}
.page-content .section-fullscreen .section-dance video{
  width: 100%;
  min-height: 800px;
  position: absolute;
  display: block;
  top: 0;
  left: 0;
  max-width: none;
  object-fit: cover;
  transform: translateX(0) translateY(0);
}

}
.page-content .section-fullscreen .large-fixed{
  position: fixed;
}
.section-dance {
  max-width: 1860px;
  margin-left: auto;
  margin-right: auto;
  text-align: center;
}


.section-fullscreen-overview .section-dance .video-fade, .section-fullscreen-overview .section-headline .section-image{
  background-size: cover;
  background-position:center center;
}
.page-content .video-fade{
position: absolute;
top: 0;
left: 0;
width: 100%;
height: 100%;
}

@media only screen and (-webkit-min-device-pixel-ratio: 1.5) and (max-width: 1068px), only screen and (max-width: 1068px) and (min-resolution: 144dpi), only screen and (-webkit-min-device-pixel-ratio: 144) and (max-width: 1068px), only screen and (max-width: 1068px) and (min-resolution: 144dppx){
.section-fullscreen-overview .section-dance .video-fade {
  background-image: url(../images/background .png);
}
}
.stopfade {
   opacity: .5;
}
.page-content .section-fullscreen-overview .section-foreground{
  opacity: 1;
}
.section-fullscreen .section-headline{
  min-height: 600px;
  height: 100vh;
  position: relative;
  z-index: 2;
}
.video .page-content .section-headline .section-image{
  background-image: none;
}
.section-fullscreen .section-headline .section-image{
  position: absolute;
  z-index: -1;
  min-height: 600px;
  height: 100vh;
  width: 100%;
}
.section-fullscreen .section-headline .section-content{
  margin-top: 0;
  margin-bottom: 0;
  position: relative;
  top: 50%;
  left:50%;
  transform: translateY(-50%);
}
h1 {
  font-size: 3rem;
  margin-top: 0;
  letter-spacing: .3rem;
}


a {
  display: inline-block;
  color: #fff;
  text-decoration: none;
  transition: .6s background;
}

.section-fullscreen .glyphicon-menu-down {
  position: absolute;
  left: 50%;
  margin: 0 auto;
  z-index: 50;
  text-align: center;
  bottom: 40px;
  width: 100px;
  height: 100px;
  top:95%;
  border: none;
  background-color: transparent;
  font-size:xx-large;
  color:#fff;
}


/*music section*/
.music-recommendation {
  height: 1200px;
  width: 100%;
  float: left;
  clear:both;
  position: relative;
  border: 0px;

}


.music-background{
  position: relative;
  height: 100%;
  top: 0;
  background-color: #a5a0a0;
}
.music-play{
  height: 12.5%;
  top: 10%;
}
.music-img{
  height:40%;
  top:10%;
 }
#myCarousel{
  border: 0;
  display: block;
  margin: auto;
  height: 350px;
  width: 400px;
  position: relative;
  max-width: 450px;
  min-width: 300px;
}
.carousel-indicators {
    position: absolute;
    bottom: 10px;
    width: 100%;
    z-index: 15;
    padding-left: 0;
    text-align: center;
    list-style: none;
    left: 0;
    margin-left: 0;
}
div.song-lyric{
  border: 0;
  display: block;
  margin: auto;
  height: 350px;
  width: 400px;
  position: relative;
  max-width: 450px;
  min-width: 300px;
}
.carousel-inner>.item>a>img, .carousel-inner>.item>img, .img-responsive, .thumbnail a>img, .thumbnail>img {
    border: 0;
    display: block;
    margin: auto;
    height: 350px;
    width: 400px;
    position: relative;
    max-width: 450px;
    min-width: 300px;
  }

.music-comment{
  position: relative;
  float: left;
  height: 40%;
}
div.sentence{
    padding-top: 200px;
    border: 0;
    display: block;
    margin: auto;
    width: 400px;
    height: 350px;
    position: relative;
}
span.comment-sentence {

    display: block;
    margin: auto;
    font-family: 'San Francisco', 'PingFang SC', Arial, Sans;
}
.btn-default{
  background-color: transparent;
  display: block;
  margin: auto;
  position: relative;

}
.audio-player,
.audio-player div,
.audio-player h1,
.audio-player a,
.audio-player img,
.audio-player span,
.audio-player button {
    margin: 0;
    padding: 0;
    border: none;
    outline: none;
}

div.audio-player {
    position: relative;
    display: block;
    margin: auto;
    width: 400px;
    height: 70px;
    max-width: 450px;
    min-width: 300px;

    background: #4c4e5a;
    background: -webkit-linear-gradient(top, #4c4e5a 0%, #2c2d33 100%);
    background: -moz-linear-gradient(top, #4c4e5a 0%, #2c2d33 100%);
    background: -o-linear-gradient(top, #4c4e5a 0%, #2c2d33 100%);
    background: -ms-linear-gradient(top, #4c4e5a 0%, #2c2d33 100%);
    background: linear-gradient(top, #4c4e5a 0%, #2c2d33 100%);

    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
}

.audio-player h1 {
    position: absolute;
    top: 20%;
    padding-left: 5px;
    font-family: Helvetica, Arial, sans-serif;
    font-weight: bold;
    font-size: 14px;
    color: #ececec;
    text-shadow: 1px 1px 1px rgba(0,0,0, .5);
}

.audio-player .cover {
    position: absolute;
    top: 0;
    left: 0;
}
#audio-player{
    width: 100%;
    height: 100%;
    position: relative;
}


